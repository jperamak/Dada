{
    "*ConfigurationName*":{
        "*VirtualKey1_Name*":[
            {
                "KeyName":"*axis or button name*",
                "IsAxis": true, //default false if property is missing
                "Inverted": true, //default false if property is missing
                "KeyTriggerCondition" : "*condition*", //(default NON_ZERO if property is missing) POSITIVE, NEGATIVE, ONE, MINUS_ONE, ABS_ONE, NON_ZERO
                "AxisOrientation" : "*orientation*" //(default NONE if property is missing) VERTICAL, HORIZONTAL, NONE
           }
        ]
    }
}






